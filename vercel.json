{
  "buildCommand": "npm run build",
  "builds": [
    {
      "src": "client/index.html",
      "use": "@vercel/static-build"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api"
    },
    {
      "source": "/(.*)",
      "destination": "/client/dist/public/$1"
    }
  ]
}
